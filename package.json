{
  "name": "tio.js",
  "version": "2.1.0",
  "description": "Unofficial node.js wrapper for the TryItOnline (tio.run) API.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run test && npx tsc --project tsconfig.json",
    "prepare": "npx prettier --write index.ts",
    "test": "npx eslint index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vierofernando/tio.js.git"
  },
  "keywords": [
    "tio",
    "eval",
    "code",
    "tryitonline",
    "api"
  ],
  "author": {
    "name": "vierofernando",
    "email": "vierofernando9@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vierofernando/tio.js/issues",
    "email": "vierofernando9@gmail.com"
  },
  "homepage": "https://github.com/vierofernando/tio.js#readme",
  "devDependencies": {
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "pre-commit": "1.2.2",
    "prettier": "2.6.1",
    "typescript": "4.6.3"
  },
  "pre-commit": {
    "run": [
      "prepare"
    ],
    "silent": true
  },
  "dependencies": {
    "undici": "5.0.0"
  },
  "type": "module"
}
