{
  "files": [
    "index.ts"
  ],
  "outFile": "index.js",
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "allowUnreachableCode": false,
    "noUnusedParameters": true,
    "strict": true,
    "strictFunctionTypes": true,
    "module": "ES2020",
    "target": "es2022",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "removeComments": true
  }
}